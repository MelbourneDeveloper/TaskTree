{
    "tasktree.sortOrder": "folder",
    "tasktree.showEmptyCategories": false,
    "tasktree.excludePatterns": [
        "**/node_modules/**",
        "**/.vscode-test/**",
        "**/vendor/**",
        "**/dist/**",
        "**/build/**",
        "**/out/**",
        "**/bin/**",
        "**/obj/**",
        "**/.git/**",
        "**/.hg/**",
        "**/.svn/**",
        "**/coverage/**",
        "**/__pycache__/**",
        "**/venv/**",
        "**/.venv/**",
        "**/env/**",
        "**/target/**",
        "**/Pods/**",
        "**/.gradle/**",
        "**/.idea/**",
        "**/.vs/**"
    ]
}
